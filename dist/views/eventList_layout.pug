doctype html
html
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1.0')
        meta(name='theme-color' content='#4DA5F4')
        meta(name='csrf-token', content=_csrf)
        title #{title} - Art Battle Vote Manager
        link(rel='shortcut icon', href=process.env.MP+'/images/favicon.ico', type='image/x-icon')
        link(rel='icon', href=process.env.MP+'/images/favicon.ico', type='image/x-icon')
        link(rel='stylesheet', href=process.env.MP+'/css/bootstrap.css')
        link(rel='stylesheet', href=process.env.MP+'/css/eventList.css?v='+process.env.BUILD_VERSION)
        link(rel="stylesheet" href="https://use.typekit.net/kot5qyf.css")
        link(rel='stylesheet', href=process.env.MP+'/css/ladda-themeless.min.css')
        link(href=process.env.MP+'/css/auction.css?v='+process.env.BUILD_VERSION, rel='stylesheet')
        link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.5.9/slick.min.css")
        link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.5.9/slick-theme.min.css")
        if title
            meta(property='og:title' content=title)
        if pageImage
            meta(property='og:image' content=pageImage)
        script.
            var mp = '#{process.env.MP}';
    body

        #koroot.container
            include partials/flash
            block content


        block scripts
            script.
                // used in vm
            script(src='https://cdn.jwplayer.com/libraries/dojeKtXD.js')
            script(src=process.env.MP+'/js/lib/jquery-3.2.1.js')
            script(src=process.env.MP+'/js/lib/bootstrap.js')
            script(src=process.env.MP+'/js/lib/knockout-3.4.2.debug.js')
            script(src=process.env.MP+'/js/common.js?v=' + process.env.BUILD_VERSION)
            script(src=process.env.MP+'/js/lib/ladda.min.js')